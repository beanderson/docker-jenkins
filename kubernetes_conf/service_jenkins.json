{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "jenkins",
    "namespace": "default",
    "labels": {
      "name": "jenkins",
      "role": "frontend"
    }
  },
  "spec": {
    "ports": [
      {
        "name": "ui",
        "protocol": "TCP",
        "port": 8080,
        "targetPort": "jenkins-http"
      },
      {
        "name": "discovery",
        "protocol": "TCP",
        "port": 50000,
        "targetPort": "jenkins-disco"
      }
    ],
    "selector": {
      "name": "jenkins",
      "role": "leader"
    }
  }
}